<Globals>
<Script>-- Define global table.
CMD = {Configuration = {Default = {}}, Stored = {}};

-- Define helper functions.
dofile("AutoPlay\\Scripts\\class.lua");
dofile("AutoPlay\\Scripts\\helper.lua");

-- Define the default configuration settings.
CMD.Configuration.Default.PromptPrefix = System.GetUserInfo().RegOwner;

-- Attempt to load configuration file.
if (File.DoesExist("AutoPlay\\config.ini")) then
	CMD.Configuration.PromptPrefix = INIFile.GetValue("AutoPlay\\config.ini", "Configuration", "PromptPrefix");
	
	if (CMD.Configuration.PromptPrefix:empty()) then
		-- Set to default.
		CMD.Configuration.PromptPrefix = CMD.Configuration.Default.PromptPrefix;
	end
else
	-- Set settings to the default ones.
	CMD.Configuration.PromptPrefix = CMD.Configuration.Default.PromptPrefix;
	
	-- Write default values.
	for K, V in pairs(CMD.Configuration.Default) do
		INIFile.SetValue("AutoPlay\\config.ini", "Configuration", K, V);
	end
end

-- Include commands.
local Commands = File.Find("AutoPlay\\Commands\\", "*.lua", false, false, nil, nil);

if (Commands) then
	for K, V in pairs(Commands) do
		dofile(V);
	end
end</Script>
